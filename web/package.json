{
  "name": "truffle-react-redux-dapp",
  "version": "0.1.0",
  "description": "truffle dapp with react/redux",
  "private": true,
  "dependencies": {
    "3box": "0.0.2",
    "@babel/polyfill": "^7.0.0",
    "babel-preset-env": "^1.7.0",
    "ethjs": "^0.4.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "web3-eth-personal": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "react-scripts": "^1.1.4",
    "react-test-renderer": "^16.3.1",
    "redux-logger": "^3.0.6",
    "regenerator-runtime": "^0.12.1",
    "sass-loader": "^7.1.0"
  },
  "scripts": {
    "babel": "babel --presets es2015 src/index.js -o build/main.bundle.js",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "scss": "node-sass --watch scss -o css",
    "lint": "eslint src --ignore-path .gitignore",
    "lint-fix": "eslint src --fix --ignore-path .gitignore"
  }
}
