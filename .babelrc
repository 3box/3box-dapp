{
  "presets": [
    "es2015",
    "react",
    "flow"
  ],
  "plugins": [
    ["babel-plugin-idx", {
      "importName": "./idx"
    }],
    "add-module-exports",
    "dynamic-import-webpack",
    "transform-regenerator",
    "transform-class-properties",
    "transform-object-rest-spread"
    ],
  "env": {
    "production": {
      "presets": ["react-optimize"]
    },
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}