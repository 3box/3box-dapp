{
  "name": "3box",
  "description": "3Box is an open source data storage solution for web3 that allows end users to manage their public and private information on the decentralized web.",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "homepage": "/",
  "repository": {
    "url": "git@github.com:uport-project/3box-dapp.git",
    "type": "git"
  },
  "scripts": {
    "compile": "truffle compile ; cp -r build/contracts/. src/contracts",
    "migrate:rinkeby": "truffle migrate --network rinkeby",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "deploy": "gh-pages -d build/application",
    "test": "node scripts/test.js --env=jsdom"
  },
